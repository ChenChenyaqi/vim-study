// 将键绑定放在此文件中以覆盖默认值
[
  {
    "key": "ctrl+[Backquote]",
    "command": "workbench.action.terminal.new"
  },
  {
    "key": "ctrl+cmd+\\",
    "command": "workbench.action.splitEditorUp"
  },
  {
    "key": "shift+left",
    "command": "vim.remap",
    "when": "vim.mode === 'Normal'",
    "args": {
      "after": [
        "<c-w>",
        "h"
      ],
    }
  },
  {
    "key": "shift+right",
    "command": "vim.remap",
    "when": "vim.mode === 'Normal'",
    "args": {
      "after": [
        "<c-w>",
        "l"
      ],
    }
  },
  {
    "key": "shift+up",
    "command": "vim.remap",
    "when": "vim.mode === 'Normal'",
    "args": {
      "after": [
        "<c-w>",
        "k"
      ],
    }
  },
  {
    "key": "shift+down",
    "command": "vim.remap",
    "when": "vim.mode === 'Normal'",
    "args": {
      "after": [
        "<c-w>",
        "j"
      ],
    }
  }
]